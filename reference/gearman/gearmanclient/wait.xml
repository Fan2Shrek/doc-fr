<refentry xml:id="gearmanclient.wait" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>GearmanClient::wait</refname>
  <refpurpose>Attend l'activité d'E/S sur toutes les connexions d'un client</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis role="GearmanClient">
   <modifier>public</modifier> <type>bool</type><methodname>GearmanClient::wait</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Cela attend l'activité de n'importe lequel des serveurs connectés.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &true; en cas de succès &false; en cas d'erreur.
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><methodname>GearmanWorker::wait</methodname></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>
